<div class="col-lg-10 ms-auto pt-5 mt-4">
  <table id="example" class="table text-dark" style="width:100%">
    <thead class="text-warning">
      <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Contact</th>

        <th>Bookings</th>
        <th>Block/Unblock</th>
      </tr>
    </thead>
    <tbody>
      {{#each users}}
      <tr>
        <td>{{this.name}}</td>
        <td>{{this.email}}</td>
        <td>{{this.number}}
        </td>

        <td>
          <!-- Button trigger modal -->
          <button type="button" class="btn btn-primary"  onclick="getBookings('{{this._id}}')">
            View
          </button>

          <!-- Modal -->
          
        </td>
        <td>
          {{#if this.isBlocked}}<button type="button" class="btn btn-success" onclick="unblockUser('{{this._id}}');"
            id="{{this._id}}">Unblock</button>
          {{else}} <button type="button" class="btn btn-danger" onclick="blockUser('{{this._id}}');"
            id="{{this._id}}">Block</button>{{/if}}

        </td>
      </tr>
      {{/each}}

      </li>

    </tbody>
