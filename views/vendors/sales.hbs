<div class="col-lg-10 ms-auto pt-5 mt-3 bg-light" >
  


    <table id="example" class="table text-dark bg-light" >
        <thead class="" style="color: #750404;">
            <tr>
                <th>Date</th>
                <th>Customer</th>
                <th>Room</th>
                <th>check In</th>
                <th>Check out</th>
                <th>Payment Mode</th>
                <th>Bill Amount</th>
                
               
            </tr>
        </thead>
        <tbody>
            {{#each bookings}}
            <tr class="mt-2">
                <td>{{this.name}}</td>
                <td>{{this.bookings.bookingDate}}</td>
                <td>{{this.bookings.category}}</td>
                <td>{{this.bookings.checkIn}}</td>
                <td>{{this.bookings.checkOut}}</td>
                <td>{{this.bookings.billAmt}}</td>
                <td>{{this.bookings.bookingStatus}}</td>
                <td> {{#if this.bookings.isActive}}
                    <button class="badge badge-success text-sm" disabled>Active</button>
                    {{else}}

                    {{#if this.bookings.cancelled}}
                    <button class="badge badge-primary text-sm" disabled>Cancelled</button>
                    {{else}}
                    {{#if this.bookings.checkedOut}}
                    Checked out

                    {{/if}}
                    {{/if}}
                    {{/if}}
                </td>
            </tr>
            {{/each}}



        </tbody>

    </table>
</div>

